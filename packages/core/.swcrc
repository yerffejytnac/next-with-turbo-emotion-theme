{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "paths": {
      "@components": ["./src/components"],
      "@primitives": ["./src/primitives"],
      "@hooks": ["./src/hooks"],
      "@tests/*": ["./src/tests/*"]
    },
    "minify": {
      "keepClassnames": true,
      "keepFnames": true
    },
    "keepClassNames": true,
    "baseUrl": ".",
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    }
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}
