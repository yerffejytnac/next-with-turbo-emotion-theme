{
  "name": "@shared/theme",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "npx swc src -d dist -s --ignore '**/*.test.ts,**/*.stories.ts'",
    "postbuild": "yarn type:emit",
    "dev": "concurrently \"yarn build --watch\" \"yarn type:emit --watch\"",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "type:check": "yarn tsc --project tsconfig.json --noEmit",
    "type:emit": "tsc --project ./tsconfig.json -d --declarationDir dist --declarationMap --emitDeclarationOnly"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "framer-motion": "^7.4.0",
    "polished": "^4.2.2",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@shared/tsconfig": "*",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "chokidar": "^3.5.3",
    "concurrently": "^7.5.0",
    "eslint": "^8.24.0",
    "eslint-config-custom": "*",
    "react": "^18.2.0",
    "swc-loader": "^0.2.3",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=14 <17"
  }
}
